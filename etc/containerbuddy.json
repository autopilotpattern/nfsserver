{
  "consul": "{{ .CONSUL }}:8500",
  "preStart": "/usr/local/bin/prestart.sh",
  "stopTimeout": 60,
  "logging": {
    "level": "INFO",
    "format": "default",
    "output": "stdout"
  },
  "services": [
    {
      "name": "nfs",
      "tags": [ "nfs" ],
      "interfaces": [
        "eth0",
        "eth1[1]",
        "192.168.0.0/16",
        "2001:db8::/64",
        "eth2:inet",
        "eth2:inet6",
        "inet",
        "inet6"
      ],
      "port": 1892,
      "health": "/usr/local/bin/health.sh",
      "poll": 3,
      "ttl": 10
    }
  ]
}
